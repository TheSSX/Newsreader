{
  "name": "Newsreader",
  "version": "0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "background.js"]
    }
  ],
  "content_security_policy":"script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "page": "start.html"
  }
}